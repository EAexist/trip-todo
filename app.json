{
    "name": "trip-todo",
    "displayName": "trip-todo",
    "expo": {
        "name": "trip-todo",
        "slug": "trip-todo",
        "scheme": "trip-todo",
        "version": "1.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "automatic",
        "icon": "./assets/images/third-party/airbnb-logo.png",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "newArchEnabled": false,
        "jsEngine": "hermes",
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "icon": "./assets/images/third-party/airbnb-logo.png",
            "package": "com.trip_todo",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/third-party/airbnb-logo.png",
                "backgroundImage": "./assets/images/third-party/airbnb-logo.png"
            },
            "allowBackup": false
        },
        "ios": {
            "icon": "./assets/images/third-party/airbnb-logo.png",
            "supportsTablet": true,
            "bundleIdentifier": "com.trip_todo"
        },
        "web": {
            "favicon": "./assets/images/third-party/airbnb-logo.png",
            "bundler": "metro"
        },
        "plugins": [
            "expo-localization",
            "expo-font",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/third-party/airbnb-logo.png",
                    "imageWidth": 300,
                    "resizeMode": "contain",
                    "backgroundColor": "#191015"
                }
            ]
        ],
        "experiments": {
            "tsconfigPaths": true
        },
        "extra": {
            "eas": {
                "projectId": "b47c3b69-2bf0-4b16-b39c-c268d8bb5ef1"
            }
        }
    },
    "ignite": {
        "version": "10.5.3"
    }
}