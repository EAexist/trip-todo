{
    "expo": {
        "name": "trip-todo",
        "slug": "trip-todo",
        "scheme": "trip-todo",
        "version": "1.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "automatic",
        "icon": "./assets/images/third-party/airbnb-logo.png",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "newArchEnabled": false,
        "jsEngine": "hermes",
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "icon": "./assets/images/third-party/airbnb-logo.png",
            "package": "com.trip_todo",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/third-party/airbnb-logo.png",
                "backgroundImage": "./assets/images/third-party/airbnb-logo.png"
            },
            "allowBackup": false
        },
        "ios": {
            "icon": "./assets/images/third-party/airbnb-logo.png",
            "supportsTablet": true,
            "bundleIdentifier": "com.trip_todo"
        },
        "web": {
            "favicon": "./assets/images/third-party/airbnb-logo.png",
            "bundler": "metro"
        },
        "plugins": [
            "expo-localization",
            [
                "@react-native-seoul/kakao-login",
                {
                    "kakaoAppKey": "db8e6a3ae272a123c55d4e1a2705c2e1",
                    "kotlinVersion": "2.0.21"
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "kotlinVersion": "2.0.21",
                        "extraMavenRepos": [
                            "https://devrepo.kakao.com/nexus/content/groups/public/"
                        ]
                    }
                }
            ],
            [
                "@react-native-google-signin/google-signin",
                {
                    "iosUrlScheme": "com.googleusercontent.apps.trip-todo"
                }
            ],
            [
                "expo-font",
                {
                    "fonts": [
                        "./assets/fonts/tossface/tossface.ttf"
                    ],
                    "android": {
                        "fonts": [
                            {
                                "fontFamily": "Pretendard",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_thin.otf",
                                        "weight": 100
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_extralight.otf",
                                        "weight": 200
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_light.otf",
                                        "weight": 300
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_regular.otf",
                                        "weight": 400
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_medium.otf",
                                        "weight": 500
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_semibold.otf",
                                        "weight": 600
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_bold.otf",
                                        "weight": 700
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_extrabold.otf",
                                        "weight": 800
                                    },
                                    {
                                        "path": "./assets/fonts/pretendard/pretendard_black.otf",
                                        "weight": 900
                                    }
                                ]
                            }
                        ]
                    },
                    "ios": {
                        "fonts": [
                            "./assets/fonts/pretendard/Pretendard-Thin.otf",
                            "./assets/fonts/pretendard/Pretendard-ExtraLight.otf",
                            "./assets/fonts/pretendard/Pretendard-Light.otf",
                            "./assets/fonts/pretendard/Pretendard-Regular.otf",
                            "./assets/fonts/pretendard/Pretendard-Medium.otf",
                            "./assets/fonts/pretendard/Pretendard-SemiBold.otf",
                            "./assets/fonts/pretendard/Pretendard-Bold.otf",
                            "./assets/fonts/pretendard/Pretendard-ExtraBold.otf",
                            "./assets/fonts/pretendard/Pretendard-Black.otf"
                        ]
                    }
                }
            ],
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/third-party/airbnb-logo.png",
                    "imageWidth": 300,
                    "resizeMode": "contain",
                    "backgroundColor": "#191015"
                }
            ]
        ],
        "experiments": {
            "tsconfigPaths": true
        },
        "extra": {
            "eas": {
                "projectId": "b47c3b69-2bf0-4b16-b39c-c268d8bb5ef1"
            }
        }
    },
    "ignite": {
        "version": "10.5.3"
    }
}