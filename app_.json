{
    "expo": {
        "name": "trip-todo",
        "slug": "trip-todo",
        "scheme": "trip-todo",
        "version": "1.0.0",
        "orientation": "portrait",
        "userInterfaceStyle": "automatic",
        "icon": "./assets/images/app/app-icon-all.png",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "newArchEnabled": false,
        "jsEngine": "hermes",
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "icon": "./assets/images/app/app-icon-android-legacy.png",
            "package": "com.trip_todo",
            "allowBackup": false
        },
        "ios": {
            "icon": "./assets/images/app/app-icon-ios.png",
            "supportsTablet": true,
            "bundleIdentifier": "com.trip_todo"
        },
        "web": {
            "favicon": "./assets/images/app/app-icon-web-favicon.png",
            "bundler": "metro"
        },
        "plugins": [
            "expo-localization",
            [
                "@react-native-seoul/kakao-login",
                {
                    "kakaoAppKey": "db8e6a3ae272a123c55d4e1a2705c2e1",
                    "kotlinVersion": "2.0.21"
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "kotlinVersion": "2.0.21",
                        "extraMavenRepos": [
                            "https://devrepo.kakao.com/nexus/content/groups/public/"
                        ]
                    }
                }
            ],
            [
                "@react-native-google-signin/google-signin",
                {
                    "iosUrlScheme": "com.googleusercontent.apps.trip-todo"
                }
            ],
            [
                "expo-font",
                {
                    "fonts": [
                        "./assets/fonts/tossface/tossface.ttf",
                        "./assets/fonts/pretendard/pretendard_thin.otf",
                        "./assets/fonts/pretendard/pretendard_extralight.otf",
                        "./assets/fonts/pretendard/pretendard_light.otf",
                        "./assets/fonts/pretendard/pretendard_regular.otf",
                        "./assets/fonts/pretendard/pretendard_medium.otf",
                        "./assets/fonts/pretendard/pretendard_semibold.otf",
                        "./assets/fonts/pretendard/pretendard_bold.otf",
                        "./assets/fonts/pretendard/pretendard_extrabold.otf",
                        "./assets/fonts/pretendard/pretendard_black.otf"
                    ],
                    "ios": {
                        "fonts": [
                            "./assets/fonts/pretendard/Pretendard-Thin.otf",
                            "./assets/fonts/pretendard/Pretendard-ExtraLight.otf",
                            "./assets/fonts/pretendard/Pretendard-Light.otf",
                            "./assets/fonts/pretendard/Pretendard-Regular.otf",
                            "./assets/fonts/pretendard/Pretendard-Medium.otf",
                            "./assets/fonts/pretendard/Pretendard-SemiBold.otf",
                            "./assets/fonts/pretendard/Pretendard-Bold.otf",
                            "./assets/fonts/pretendard/Pretendard-ExtraBold.otf",
                            "./assets/fonts/pretendard/Pretendard-Black.otf"
                        ]
                    }
                }
            ],
            [
                "expo-xml-font",
                [
                    {
                        "name": "Pretendard",
                        "folder": "assets/fonts/pretendard",
                        "variants": [
                            {
                                "fontFile": "pretendard_thin.otf",
                                "fontWeight": 100
                            },
                            {
                                "fontFile": "pretendard_extralight.otf",
                                "fontWeight": 200
                            },
                            {
                                "fontFile": "pretendard_light.otf",
                                "fontWeight": 300
                            },
                            {
                                "fontFile": "pretendard_regular.otf",
                                "fontWeight": 400
                            },
                            {
                                "fontFile": "pretendard_medium.otf",
                                "fontWeight": 500
                            },
                            {
                                "fontFile": "pretendard_semibold.otf",
                                "fontWeight": 600
                            },
                            {
                                "fontFile": "pretendard_bold.otf",
                                "fontWeight": 700
                            },
                            {
                                "fontFile": "pretendard_extrabold.otf",
                                "fontWeight": 800
                            },
                            {
                                "fontFile": "pretendard_black.otf",
                                "fontWeight": 900
                            }
                        ]
                    }
                ]
            ],
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/third-party/airbnb-logo.png",
                    "imageWidth": 300,
                    "resizeMode": "contain",
                    "backgroundColor": "#191015"
                }
            ]
        ],
        "experiments": {
            "tsconfigPaths": true
        },
        "extra": {
            "eas": {
                "projectId": "b47c3b69-2bf0-4b16-b39c-c268d8bb5ef1"
            }
        }
    },
    "ignite": {
        "version": "10.5.3"
    }
}